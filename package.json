{
  "name": "monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "studio:production": "cross-env NODE_ENV=production npx --prefix ./packages/db drizzle-kit studio --host 127.0.0.1",
    "studio:development": "cross-env NODE_ENV=development npx --prefix ./packages/db drizzle-kit studio --host 127.0.0.1",
    "migrate:production": "cross-env NODE_ENV=production npx --prefix ./packages/db drizzle-kit generate && cross-env NODE_ENV=production node -r esbuild-register ./packages/db/migrate.ts",
    "migrate:development": "cross-env NODE_ENV=development npx --prefix ./packages/db drizzle-kit generate && cross-env NODE_ENV=development node -r esbuild-register ./packages/db/migrate.ts"
  },
  "devDependencies": {
    "@types/node": "^20.14.1",
    "@types/pg": "^8.11.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.1.0",
    "turbo": "^1.12.3"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@lexical/headless": "^0.14.5",
    "@lexical/html": "^0.14.5",
    "@lexical/link": "^0.14.5",
    "@lexical/list": "^0.14.5",
    "@lexical/react": "^0.14.5",
    "@lexical/rich-text": "^0.14.5",
    "@neynar/nodejs-sdk": "^1.18.0",
    "@pinata/sdk": "^2.1.0",
    "@privy-io/react-auth": "^1.67.0",
    "@privy-io/server-auth": "^1.7.3",
    "@privy-io/wagmi": "^0.2.9",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tailwindcss/typography": "^0.5.12",
    "@uidotdev/usehooks": "^2.4.1",
    "arweave": "^1.14.4",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.21.2",
    "drizzle-orm": "^0.30.10",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "frog": "^0.11.5",
    "jiti": "^1.21.0",
    "lexical": "^0.14.5",
    "next": "^14.2.1",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "phosphor-react-sc": "^1.0.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-linkify-it": "^1.0.8",
    "react-tweet": "^3.2.0",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "viem": "^2.10.1",
    "wagmi": "^2.8.1",
    "zod": "^3.23.0"
  }
}
