{
  "linter": {
    "rules": {
      "suspicious": {
        "noExplicitAny": "off",
        "noArrayIndexKey": "off"
      },
      "a11y": {
        "useIframeTitle": "off",
        "useButtonType": "off",
        "useAltText": "off",
        "noSvgWithoutTitle": "off",
        "useKeyWithClickEvents": "off"
      },
      "style": {
        "useNumberNamespace": "off",
        "useNodejsImportProtocol": "off",
        "useTemplate": "off"
      },
      "correctness": {
        "useExhaustiveDependencies": "off",
        "useJsxKeyInIterable": "off"
      }
    }
  }
}
